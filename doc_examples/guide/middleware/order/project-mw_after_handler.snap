use pavex::blueprint::router::GET;
use pavex::blueprint::Blueprint;
use pavex::f;

pub fn blueprint() -> Blueprint {
    let mut bp = Blueprint::new();
    bp.wrap(f!(crate::wrap1));
    bp.route(GET, "/", f!(super::handler));
    bp.wrap(f!(crate::wrap2));
    bp
}
